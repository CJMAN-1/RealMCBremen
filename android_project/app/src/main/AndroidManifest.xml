<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unit01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity android:name="com.example.unit01.MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.unit01.Piano_class"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name="com.example.unit01.Guitar_class"
            android:screenOrientation="landscape">
        </activity>
        <activity
        android:name="com.example.unit01.Maracas_class"
        android:screenOrientation="landscape">
        </activity>
        <activity
            android:name="com.example.unit01.Playlist_class"
            android:screenOrientation="landscape">
        </activity>
    </application>

</manifest>